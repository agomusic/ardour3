#include <gtk/gtkcolorbutton.h>
#include <gdkmm/color.h>

namespace Gtk
{

Gdk::Color ColorButton::get_color() const
{
  Gdk::Color color;
  gtk_color_button_get_color(const_cast<GtkColorButton*>(gobj()), color.gobj());
  return color;
}

  

} // namespace Gtk

