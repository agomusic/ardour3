#: ../ardour_ui.cc:1945
msgid "ardour: cleanup"
msgstr "ardour: rensning"

#: ../ardour_ui.cc:1981 ../ardour_ui.cc:1987
msgid "files were"
msgstr "filer"

#: ../ardour_ui.cc:1983 ../ardour_ui.cc:1989
msgid "file was"
